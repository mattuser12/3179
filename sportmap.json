{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/mattuser12/3179/main/data/Summer%20Olympics%20sport.csv"},
    "transform": [{
      "filter": {"and": [
        {"field": "IMDB Rating", "valid": true},
        {"field": "Rotten Tomatoes Rating", "valid": true}
      ]}
    }],
    "mark": "rect",
    "width": 300,
    "height": 200,
    "encoding": {
      "x": {
        "bin": {"maxbins":60},
        "field": "IMDB Rating",
        "type": "quantitative"
      },
      "y": {
        "bin": {"maxbins": 40},
        "field": "Rotten Tomatoes Rating",
        "type": "quantitative"
      },
      "color": {
        "aggregate": "count",
        "type": "quantitative"
      }
    },
    "config": {
      "view": {
        "stroke": "transparent"
      }
    }
  }
  